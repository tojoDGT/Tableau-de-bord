.nav-side-menu {
  background: linear-gradient(90deg, #2e4376, #6d7b95);
  color: #e1ffff!important;
}
.nav-side-menu .brand {
  background-color: #9aa2b5!important;
  line-height: 50px!important;
  display: block!important;
  text-align: center!important;
  font-size: 14px!important;
}
.nav-side-menu .toggle-btn {
  display: none!important;
}
.nav-side-menu ul,
.nav-side-menu li {
  list-style: none!important;
  padding: 0px!important;
  margin: 0px!important;
  line-height: 35px!important;
  cursor: pointer!important;
  text-align:left!important;
}

#leftContent ul li a span {
    display: inline;
}

#leftContent ul li {
	border-right: none!important;
}

.leftMenuRohi {
	min-height:700px;
	margin-top:30px!important;
}
.nav-side-menu ul :not(collapsed) .arrow:before,
.nav-side-menu li :not(collapsed) .arrow:before {
  font-family: FontAwesome!important;
  content: "\f078"!important;
  display: inline-block!important;
  padding-left: 10px!important;
  padding-right: 10px!important;
  vertical-align: middle!important;
  float: right!important;
}
.nav-side-menu ul .active,
.nav-side-menu li .active {
  /*border-left: 3px solid #d19b3d!important;
  background-color: #72471e!important;*/
  border-left: 3px solid #a893e1!important;
  background-color: #8b78c0!important
  /*background: linear-gradient(90deg, #673202, #be8d65);*/
}
.nav-side-menu ul .sub-menu li.active,
.nav-side-menu li .sub-menu li.active {
  color: #835ee7!important
}
.nav-side-menu ul .sub-menu li.active a,
.nav-side-menu li .sub-menu li.active a {
  color: #835ee7!important
}
.nav-side-menu ul .sub-menu li,
.nav-side-menu li .sub-menu li {
  background-color: #1d3942!important;
  border: none!important;
  line-height: 28px!important;
  border-bottom: 1px solid #43575d!important;
  margin-left: 0px!important;
}
.nav-side-menu ul .sub-menu li:hover,
.nav-side-menu li .sub-menu li:hover {
  background-color: #132930!important
}
.nav-side-menu ul .sub-menu li:before,
.nav-side-menu li .sub-menu li:before {
  font-family: FontAwesome!important;
  content: "\f105"!important;
  display: inline-block!important;
  padding-left: 10px!important;
  padding-right: 10px!important;
  vertical-align: middle!important;
  border-left: 3px solid #456874!important;
}
.nav-side-menu li {
  padding-left: 0px!important;
  border-left: 3px solid #9aa2b5!important;
  border-bottom: 1px solid #23282e!important;
}
.nav-side-menu li a {
  text-decoration: none!important;
  color: #e1ffff!important;
}
.nav-side-menu li a i {
  padding-left: 10px!important;
  width: 20px!important;
  padding-right: 20px!important;
}
.nav-side-menu li:hover {
  border-left: 3px solid #d19b3d!important;
  background-color: #4f5b69!important;
  -webkit-transition: all 1s ease!important;
  -moz-transition: all 1s ease!important;
  -o-transition: all 1s ease!important;
  -ms-transition: all 1s ease!important;
  transition: all 1s ease!important;
}

.nav-side-menu .menu-list .menu-content {
    display: block!important;
 }